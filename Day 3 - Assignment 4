public class TypeCastingExample {
    public static void main(String[] args) {
        
        int myInt = 5;
        double myDouble = myInt; 
        System.out.println("Widening casting example:");
        System.out.println("myInt value: " + myInt);
        System.out.println("myDouble value: " + myDouble);
        System.out.println();

      
        double myNewDouble = 3.14;
        int myNewInt = (int) myNewDouble; 
        System.out.println("Narrowing casting example:");
        System.out.println("myNewDouble value: " + myNewDouble);
        System.out.println("myNewInt value: " + myNewInt);
    }
}
